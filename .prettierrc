{
  "tabWidth": 2,
  "printWidth": 100,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "cssDeclarationSorterOrder": "alphabetical",
  "cssDeclarationSorterKeepOverrides": true,
  "plugins": ["prettier-plugin-css-order"]
}
